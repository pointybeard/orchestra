{
    "name": "orchestra/orchestra",
    "description": "Meta package for scaffolding and rapidly deploying Symphony CMS builds",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Alannah Kearney",
            "email": "hi@alannahkearney.com",
            "homepage": "http://alannahkearney.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/pointybeard/orchestra/issues"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "pointybeard/helpers": "~1.2.0",
        "pointybeard/symphony-pdo": "~0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Orchestra\\Orchestra\\": "src/Orchestra/"
        },
        "files": [
            "src/Orchestra/Orchestra.php"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
