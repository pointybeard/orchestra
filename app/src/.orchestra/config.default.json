{
    "symphony": {
        "admin-path": "admin",
        "pagination_maximum_rows": 50,
        "association_maximum_rows": 5,
        "lang": "en",
        "pages_table_nest_children": "no",
        "version": "2.7.10",
        "cookie_prefix": "orchestra-",
        "session_gc_divisor": 10,
        "cell_truncation_length": 75,
        "enable_xsrf": "yes",
        "error_reporting_all": "no"
    },
    "log": {
        "archive": 1,
        "maxsize": 102400,
        "classpath": "\\Log"
    },
    "database": {
        "host": null,
        "port": null,
        "user": null,
        "password": null,
        "db": null,
        "tbl_prefix": null,
        "query_caching": true,
        "query_logging": true,
        "character_encoding": "utf8mb4",
        "character_set": "utf8mb4",
        "strip_engine": true,
        "strip_default_collate": true,
        "strip_default_charset": true,
        "strip_auto_increment": true
    },
    "public": {
        "display_event_xml_in_source": "no"
    },
    "general": {
        "sitename": null,
        "useragent": "Orchestra/1.0.0"
    },
    "file": {
        "write_mode": "0644"
    },
    "directory": {
        "write_mode": "0755"
    },
    "region": {
        "time_format": "g:i a",
        "date_format": "m/d/Y",
        "datetime_separator": " ",
        "timezone": "Australia/Brisbane"
    },
    "cache_driver": {
        "default": "database"
    },
    "xslt": {
        "processor": "\\XsltProcess"
    },
    "admin": {
        "max_upload_size": 104857600,
        "upload_blacklist": "/\\\\.(?:php[34567s]?|phtml)$/i"
    },
    "sorting": {}
}
